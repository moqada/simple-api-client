{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "lib": ["es6", "dom"],
    "baseUrl": "typescript",
    "sourceMap": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  }
}

